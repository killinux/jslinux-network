<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule "hda(.*).bin" "http://%{SERVER_NAME}/%{REQUEST_URI}/../disk-expander.php?index=$1" [P,L]
RewriteRule "hdb(.*).bin" "http://%{SERVER_NAME}/%{REQUEST_URI}/../disk-expander.php?index=$1&hdb=true" [P,L]
RewriteRule "^offlinemanifest.appcache$" "http://%{SERVER_NAME}/%{REQUEST_URI}/../offlinemanifest.php" [P,L]
RewriteRule "index.html" "http://%{SERVER_NAME}/%{REQUEST_URI}/../index.php" [P,L]
</IfModule>
